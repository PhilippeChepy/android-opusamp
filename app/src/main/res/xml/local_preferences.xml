<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <Preference
        android:key="location"
        android:title="@string/label_library_settings_location"
        android:icon="@drawable/ic_action_tiles_large" />


    <PreferenceScreen
        android:title="@string/header_interface"
        android:icon="@drawable/ic_action_tiles_large" >

        <PreferenceCategory android:title="@string/loc_preference_collection_screen_title" >
            <eu.chepy.audiokit.utils.support.android.preference.MultiSelectListPreference
                android:capitalize="words"
                android:defaultValue="@array/tab_titles"
                android:dialogTitle="@string/loc_preference_collection_tab_visibility_dialog_title"
                android:entries="@array/tab_titles"
                android:entryValues="@array/tab_titles"
                android:key="@string/key_tabs_enabled"
                android:summary="@string/loc_preference_collection_tab_visibility_summary"
                android:title="@string/loc_preference_collection_tab_visibility_dialog_title" />
        </PreferenceCategory>

    </PreferenceScreen>

</PreferenceScreen>